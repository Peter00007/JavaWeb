<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>SimpleServlet</servlet-name>
        <servlet-class>com.example.simple_example.SimpleServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>StatusCodeDemo</servlet-name>
        <servlet-class>com.example.http_code.StatusCodeDemo</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ServletRequestDemo</servlet-name>
        <servlet-class>com.example.request.ServletRequestDemo</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ServletResponseDemo</servlet-name>
        <servlet-class>com.example.response.ServletResponseDemo</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>PassingParametersUsingGetMethodDemo</servlet-name>
        <servlet-class>com.example.passing.PassingParametersUsingGetMethodDemo</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>PassingParametersUsingPostMethodDemo</servlet-name>
        <servlet-class>com.example.passing.PassingParametersUsingPostMethodDemo</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ErrorDemo</servlet-name>
        <servlet-class>com.example.error.ErrorDemo</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>SessionTrackingDemo</servlet-name>
        <servlet-class>com.example.session.SessionTrackingDemo</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>CookieDemo</servlet-name>
        <servlet-class>com.example.cookie.CookieDemo</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>DateTimeDemo</servlet-name>
        <servlet-class>com.example.date_time.DateTimeDemo</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>AutoRefreshDemo</servlet-name>
        <servlet-class>com.example.auto_refresh.AutoRefreshDemo</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>RedirectDemo</servlet-name>
        <servlet-class>com.example.redirect.RedirectDemo</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>PageVisitsCounterDemo</servlet-name>
        <servlet-class>com.example.counter.PageVisitsCounterDemo</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>LocaleDemo</servlet-name>
        <servlet-class>com.example.locale.LocaleDemo</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>SimpleServlet</servlet-name>
        <url-pattern>/SimpleServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>StatusCodeDemo</servlet-name>
        <url-pattern>/StatusCodeDemo</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ServletRequestDemo</servlet-name>
        <url-pattern>/ServletRequestDemo</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ServletResponseDemo</servlet-name>
        <url-pattern>/ServletResponseDemo</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>PassingParametersUsingGetMethodDemo</servlet-name>
        <url-pattern>/PassingParametersUsingGetMethodDemo</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>PassingParametersUsingPostMethodDemo</servlet-name>
        <url-pattern>/PassingParametersUsingPostMethodDemo</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ErrorDemo</servlet-name>
        <url-pattern>/ErrorDemo</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>SessionTrackingDemo</servlet-name>
        <url-pattern>/SessionTrackingDemo</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>CookieDemo</servlet-name>
        <url-pattern>/CookieDemo</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>DateTimeDemo</servlet-name>
        <url-pattern>/DateTimeDemo</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>AutoRefreshDemo</servlet-name>
        <url-pattern>/AutoRefreshDemo</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>RedirectDemo</servlet-name>
        <url-pattern>/RedirectDemo</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>PageVisitsCounterDemo</servlet-name>
        <url-pattern>/PageVisitsCounterDemo</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>LocaleDemo</servlet-name>
        <url-pattern>/LocaleDemo</url-pattern>
    </servlet-mapping>
</web-app>
